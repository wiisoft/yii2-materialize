{
  "name": "wiisoft/yii2-materialize",
  "description": "This is a set of widgets based on an improved version of MAZA CSS framework - MaterializeCSS framework for Yii2",
  "keywords": ["yii2", "materialize","material-design","yii2-extensions","yii2-design"],
  "type": "yii2-extension",
  "license": "MIT",
  "support": {
    "issues": "https://github.com/wiisoft/yii2-materialize/issues",
    "source": "https://github.com/wiisoft/yii2-materialize"
  },
  "authors": [
    {
      "name": "wiisoft",
      "email": "webdev@webmaker.today"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "yiisoft/yii2": "*",
    "bower-asset/maza": "@dev"
  },
  "autoload": {
    "psr-4": {
      "wii\\materialize\\": ""
    }
  },
  "extra": {
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower"
    }
  }
}